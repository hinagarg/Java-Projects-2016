<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>JSPExample</display-name>
  <context-param>
    <param-name>email</param-name>
    <param-value>admin@abc.com</param-value>
  </context-param>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>PaymentHistory</display-name>
    <servlet-name>PaymentHistory</servlet-name>
    <servlet-class>com.training.servlet.PaymentHistory</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PaymentHistory</servlet-name>
    <url-pattern>/PaymentHistory</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Redirect</display-name>
    <servlet-name>Redirect</servlet-name>
    <servlet-class>com.training.servlet.Redirect</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Redirect</servlet-name>
    <url-pattern>/Redirect</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ValidateLoginServlet</display-name>
    <servlet-name>ValidateLoginServlet</servlet-name>
    <servlet-class>com.training.servlet.ValidateLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ValidateLoginServlet</servlet-name>
    <url-pattern>/ValidateLoginServlet</url-pattern>
  </servlet-mapping>
</web-app>